{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV",
    "NEXT_PUBLIC_APP_HOST",
    "NEXT_PUBLIC_APP_PORT",
    "NEXT_PUBLIC_APP_URL",
    "NEXT_PUBLIC_APP_SERVER_HOST",
    "NEXT_PUBLIC_APP_SERVER_PORT",
    "NEXT_PUBLIC_APP_SERVER_URL",
    "NEXT_PUBLIC_KAKAO_API_KEY",
    "APP_HOST",
    "APP_PORT",
    "APP_URL",
    "APP_CLIENT_HOST",
    "APP_CLIENT_PORT",
    "APP_CLIENT_URL",
    "APP_CDN_URL",
    "EMAIL_HOST",
    "EMAIL_USER",
    "EMAIL_SECRET",
    "JWT_SECRET",
    "JWT_REFRESH_SECRET",
    "KAKAO_CLIENT_ID",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "NAVER_CLIENT_ID",
    "NAVER_CLIENT_SECRET",
    "AWS_REGION",
    "AWS_BUCKET_NAME",
    "AWS_S3_ACCESS_KEY",
    "AWS_S3_SECRET_ACCESS_KEY"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^db:generate", "^build"]
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "lint": {}
  }
}

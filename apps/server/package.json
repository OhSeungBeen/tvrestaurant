{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "dev": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rm -rf .turbo node_modules"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.481.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "@ts-rest/core": "^3.30.4",
    "@ts-rest/nest": "^3.30.4",
    "@tvrestaurant/contracts": "workspace:*",
    "@tvrestaurant/database": "workspace:*",
    "@types/shortid": "^0.0.32",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.6",
    "joi": "^17.9.2",
    "nodemailer": "^6.6.3",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-naver-v2": "^2.0.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "shortid": "^2.2.16",
    "uuid": "^9.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@tvrestaurant/eslint-config-nestjs": "workspace:*",
    "@tvrestaurant/prettier-config": "workspace:*",
    "@tvrestaurant/ts-config": "workspace:*",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "eslint": "^8.52.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.20",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
